  <xacro:include filename="$(find abb_resources)/urdf/common_materials.xacro"/>
  
  <xacro:macro name="abb_irb6640_185_280" params="prefix">
    <!-- link list -->
    
    <link name="${prefix}base_link">
     <inertial>
        <mass value="6.215"/>
        <origin xyz="-0.04204 8.01E-05 0.07964" rpy="0 0 0"/>
