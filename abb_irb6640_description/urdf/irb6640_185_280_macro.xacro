        </geometry>
        <xacro:material_abb_orange />
      </visual>
    </link>
    
    
    <link name="${prefix}link_2">
      <inertial>
        <mass value="3.909"/>
        <origin xyz="0.00078 -0.00212 0.10124" rpy="0 0 0"/>
        <inertia ixx="0.0603111" ixy="9.83431E-06" ixz="5.72407E-05" iyy="0.041569" iyz="-0.00050497" izz="0.0259548"/>
      </inertial>
      <collision name="collision">
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://abb_irb6640_description/meshes/irb6640_185_280/collision/link_2.stl"/>
        </geometry>
        <xacro:material_abb_yellow />
      </collision>
      <visual name="visual">
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://abb_irb6640_description/meshes/irb6640_185_280/visual/link_2.dae"/>
        </geometry>
        <xacro:material_abb_orange />
      </visual>
    </link>
    
    
    
    <link name="${prefix}link_3">
      <inertial>
        <mass value="2.944"/>
        <origin xyz="0.02281 0.00106 0.05791" rpy="0 0 0"/>
        <inertia ixx="0.00835606" ixy="-8.01545E-05" ixz="0.00142884" iyy="0.016713" iyz="-0.000182227" izz="0.0126984"/>
      </inertial>
      <collision name="collision">
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>          
          <mesh filename="package://abb_irb6640_description/meshes/irb6640_185_280/collision/link_3.stl"/>
        </geometry>
        <xacro:material_abb_yellow />
      </collision>
      <visual name="visual">
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://abb_irb6640_description/meshes/irb6640_185_280/visual/link_3.dae"/>
        </geometry>
        <xacro:material_abb_orange />
      </visual>
    </link>
    
    
    
    <link name="${prefix}link_4">
      <inertial>
        <mass value="1.328"/>
        <origin xyz="0.2247 0.00015 0.00041" rpy="0 0 0"/>
        <inertia ixx="0.00284661" ixy="-2.12765E-05" ixz="-1.6435E-05" iyy="0.00401346" iyz="1.31336E-05" izz="0.0052535"/>
      </inertial>
      <collision name="collision">
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <mesh filename="package://abb_irb6640_description/meshes/irb6640_185_280/collision/link_4.stl"/>
        </geometry>
        <xacro:material_abb_yellow />
      </collision>
