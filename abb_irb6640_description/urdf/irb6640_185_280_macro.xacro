        </geometry>
        <xacro:material_abb_black />
        <material name="yellow"/>
      </visual>
    </link>
    <link name="${prefix}tool0"/>

    <!--Cylinder and piston -->
    <link name="${prefix}link_cylinder">
      <collision name="collision">
        <geometry>
          <mesh filename="package://abb_irb6640_description/meshes/irb6640_185_280/collision/cylinder_link.stl"/>
        </geometry>
      </collision>
      <visual name="visual">
        <geometry>
          <mesh filename="package://abb_irb6640_description/meshes/irb6640_185_280/visual/cylinder_link.dae"/>
        </geometry>
